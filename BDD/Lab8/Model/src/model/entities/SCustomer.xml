<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE Entity SYSTEM "jbo_03_01.dtd">
<!---->
<Entity
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SCustomer"
  Version="11.1.2.60.81"
  DBObjectType="table"
  DBObjectName="S_CUSTOMER"
  AliasName="SCustomer"
  BindingStyle="OracleName"
  UseGlueCode="false"
  xmlns:validation="http://xmlns.oracle.com/adfm/validation">
  <Attribute
    Name="Id"
    IsNotNull="true"
    Precision="7"
    Scale="0"
    ColumnName="ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="S_CUSTOMER"
    PrimaryKey="true"/>
  <Attribute
    Name="Name"
    IsNotNull="true"
    Precision="50"
    ColumnName="NAME"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="Phone"
    Precision="25"
    ColumnName="PHONE"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
    <validation:RegExpValidationBean
      Name="Phone_Rule_1"
      ResId="model.entities.SCustomer.Phone_Rule_1"
      OnAttribute="Phone"
      Pattern="([1-9]{3})[1-9]{3}-[1-9]{4}"
      Inverse="false"/>
  </Attribute>
  <Attribute
    Name="Address"
    Precision="400"
    ColumnName="ADDRESS"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="400"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="City"
    Precision="30"
    ColumnName="CITY"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <validation:CompareValidationBean
      Name="City_Rule_0"
      ResId="model.entities.SCustomer.City_Rule_0"
      OnAttribute="City"
      OperandType="LITERAL"
      Inverse="false"
      CompareType="EQUALTO"
      CompareValue="Prague 
Buffallo 
Alexandria 
Cannes"/>
  </Attribute>
  <Attribute
    Name="State"
    Precision="20"
    ColumnName="STATE"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="Country"
    Precision="30"
    ColumnName="COUNTRY"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="ZipCode"
    Precision="75"
    ColumnName="ZIP_CODE"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="75"/>
    </DesignTime>
    <validation:RegExpValidationBean
      Name="ZipCode_Rule_0"
      ResId="model.entities.SCustomer.ZipCode_Rule_1"
      OnAttribute="ZipCode"
      Pattern="[a-z][1-9][a-z] [1-9][a-z][1-9]"
      Inverse="false"/>
  </Attribute>
  <Attribute
    Name="CreditRating"
    Precision="9"
    ColumnName="CREDIT_RATING"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="9"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="SalesRepId"
    Precision="7"
    Scale="0"
    ColumnName="SALES_REP_ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="S_CUSTOMER"/>
  <Attribute
    Name="RegionId"
    Precision="7"
    Scale="0"
    ColumnName="REGION_ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="S_CUSTOMER">
    <validation:CompareValidationBean
      Name="RegionId_Rule_0"
      OnAttribute="RegionId"
      OperandType="JBO"
      Inverse="false"
      CompareType="EQUALTO"
      CompareValue="model.views.SRegionView.Id"
      ResId="model.entities.SCustomer.RegionId_Rule_1"/>
  </Attribute>
  <Attribute
    Name="Comments"
    Precision="255"
    ColumnName="COMMENTS"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="S_CUSTOMER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="255"/>
    </DesignTime>
  </Attribute>
  <AccessorAttribute
    Name="SEmp"
    Association="model.entities.assoc.SSalesRepIdFkAssoc"
    AssociationEnd="model.entities.assoc.SSalesRepIdFkAssoc.SEmp"
    AssociationOtherEnd="model.entities.assoc.SSalesRepIdFkAssoc.SCustomer"
    Type="oracle.jbo.server.EntityImpl"
    IsUpdateable="true"/>
  <AccessorAttribute
    Name="SOrd"
    Association="model.entities.assoc.SOrdCustomerIdFkAssoc"
    AssociationEnd="model.entities.assoc.SOrdCustomerIdFkAssoc.SOrd"
    AssociationOtherEnd="model.entities.assoc.SOrdCustomerIdFkAssoc.SCustomer"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <AccessorAttribute
    Name="SRegion"
    Association="model.entities.assoc.SCustomerRegionIdFkAssoc"
    AssociationEnd="model.entities.assoc.SCustomerRegionIdFkAssoc.SRegion"
    AssociationOtherEnd="model.entities.assoc.SCustomerRegionIdFkAssoc.SCustomer"
    Type="oracle.jbo.server.EntityImpl"
    IsUpdateable="true"/>
  <Key
    Name="SCustomerCreditRatingCk">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="S_CUSTOMER_CREDIT_RATING_CK"/>
      <Attr Name="_checkCondition"
        Value="CREDIT_RATING IN ('EXCELLENT', 'GOOD', 'POOR')"/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="SCustomerIdNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="S_CUSTOMER_ID_NN"/>
      <Attr Name="_checkCondition" Value="ID IS NOT NULL"/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="SCustomerIdPk"
    PrimaryKey="true">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="S_CUSTOMER_ID_PK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="model.entities.SCustomer.Id"/>
    </AttrArray>
  </Key>
  <Key
    Name="SCustomerNameNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="S_CUSTOMER_NAME_NN"/>
      <Attr Name="_checkCondition" Value="NAME IS NOT NULL"/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="SCustomerRegionIdFk">
    <DesignTime>
      <Attr Name="_referencedKey" Value="S_REGION_ID_PK"/>
      <Attr Name="_isForeign" Value="true"/>
      <Attr Name="_DBObjectName" Value="S_CUSTOMER_REGION_ID_FK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="model.entities.SCustomer.RegionId"/>
    </AttrArray>
  </Key>
  <Key
    Name="SSalesRepIdFk">
    <DesignTime>
      <Attr Name="_referencedKey" Value="S_EMP_ID_PK"/>
      <Attr Name="_isForeign" Value="true"/>
      <Attr Name="_DBObjectName" Value="S_SALES_REP_ID_FK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="model.entities.SCustomer.SalesRepId"/>
    </AttrArray>
  </Key>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</Entity>
