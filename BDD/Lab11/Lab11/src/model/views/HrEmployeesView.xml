<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="HrEmployeesView"
  Version="11.1.2.60.81"
  SelectList="HrEmployees.FIRST_NAME, 
       HrEmployees.LAST_NAME, 
       HrEmployees.HIRE_DATE, 
       HrEmployees.SALARY, 
       HrEmployees.ROWID, 
       HrJobs.JOB_TITLE, 
       HrLocations.CITY, 
       HrLocations.LOCATION_ID, 
       HrLocations.STATE_PROVINCE, 
       HrJobs.JOB_ID, 
       HrCountries.COUNTRY_NAME, 
       HrCountries.COUNTRY_ID, 
       HrDepartments.DEPARTMENT_NAME, 
       HrDepartments.DEPARTMENT_ID"
  FromList="HR_EMPLOYEES HrEmployees, HR_JOBS HrJobs, HR_LOCATIONS HrLocations, HR_REGIONS HrRegions, HR_DEPARTMENTS HrDepartments, HR_COUNTRIES HrCountries"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  Where="(HrLocations.LOCATION_ID = HrDepartments.LOCATION_ID(+)) AND (HrLocations.COUNTRY_ID = HrCountries.COUNTRY_ID)">
  <EntityUsage
    Name="HrEmployees"
    Entity="model.entities.HrEmployees"/>
  <EntityUsage
    Name="HrJobs"
    Entity="model.entities.HrJobs"
    SourceUsage="model.views.HrEmployeesView.HrEmployees"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="HrLocations"
    Entity="model.entities.HrLocations"
    SourceUsage="model.views.HrEmployeesView.HrJobs"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="HrRegions"
    Entity="model.entities.HrRegions"
    SourceUsage="model.views.HrEmployeesView.HrLocations"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="HrDepartments"
    Entity="model.entities.HrDepartments"
    Association="model.entities.assoc.DeptLocFkAssoc"
    AssociationEnd="model.entities.assoc.DeptLocFkAssoc.HrDepartments"
    SourceUsage="model.views.HrEmployeesView.HrLocations"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"
    JoinType="LEFT OUTER JOIN">
    <DesignTime>
      <AttrArray Name="_srcAttributes">
        <Item Value="model.entities.HrLocations.LocationId"/>
      </AttrArray>
      <AttrArray Name="_dstAttributes">
        <Item Value="model.entities.HrDepartments.LocationId"/>
      </AttrArray>
    </DesignTime>
  </EntityUsage>
  <EntityUsage
    Name="HrCountries"
    Entity="model.entities.HrCountries"
    Association="model.entities.assoc.LocCIdFkAssoc"
    AssociationEnd="model.entities.assoc.LocCIdFkAssoc.HrCountries"
    SourceUsage="model.views.HrEmployeesView.HrLocations"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"
    JoinType="INNER JOIN">
    <DesignTime>
      <AttrArray Name="_srcAttributes">
        <Item Value="model.entities.HrLocations.CountryId"/>
      </AttrArray>
      <AttrArray Name="_dstAttributes">
        <Item Value="model.entities.HrCountries.CountryId"/>
      </AttrArray>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
    Name="FirstName"
    PrecisionRule="true"
    EntityAttrName="FirstName"
    EntityUsage="HrEmployees"
    AliasName="FIRST_NAME"/>
  <ViewAttribute
    Name="LastName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LastName"
    EntityUsage="HrEmployees"
    AliasName="LAST_NAME"/>
  <ViewAttribute
    Name="HireDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="HireDate"
    EntityUsage="HrEmployees"
    AliasName="HIRE_DATE"
    IsUpdateable="false">
    <Properties>
      <SchemaBasedProperties>
        <FMT_FORMATTER
          ResId="model.views.HrEmployeesView.HireDate_FMT_FORMATTER"/>
        <FMT_FORMAT
          ResId="model.views.HrEmployeesView.HireDate_FMT_FORMAT"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Salary"
    PrecisionRule="true"
    EntityAttrName="Salary"
    EntityUsage="HrEmployees"
    AliasName="SALARY"/>
  <ViewAttribute
    Name="Rowid"
    IsUpdateable="false"
    PrecisionRule="true"
    EntityAttrName="Rowid"
    EntityUsage="HrEmployees"
    AliasName="ROWID"/>
  <ViewAttribute
    Name="JobTitle"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="JobTitle"
    EntityUsage="HrJobs"
    AliasName="JOB_TITLE"
    IsQueriable="false">
    <Properties>
      <SchemaBasedProperties>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="City"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="City"
    EntityUsage="HrLocations"
    AliasName="CITY"/>
  <ViewAttribute
    Name="LocationId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LocationId"
    EntityUsage="HrLocations"
    AliasName="LOCATION_ID"/>
  <ViewAttribute
    Name="StateProvince"
    PrecisionRule="true"
    EntityAttrName="StateProvince"
    EntityUsage="HrLocations"
    AliasName="STATE_PROVINCE"/>
  <ViewAttribute
    Name="JobId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="JobId"
    EntityUsage="HrJobs"
    AliasName="JOB_ID">
    <Properties>
      <SchemaBasedProperties>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CountryName"
    PrecisionRule="true"
    EntityAttrName="CountryName"
    EntityUsage="HrCountries"
    AliasName="COUNTRY_NAME"/>
  <ViewAttribute
    Name="CountryId"
    PrecisionRule="true"
    EntityAttrName="CountryId"
    EntityUsage="HrCountries"
    AliasName="COUNTRY_ID"
    IsNotNull="true"/>
  <ViewAttribute
    Name="DepartmentName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="DepartmentName"
    EntityUsage="HrDepartments"
    AliasName="DEPARTMENT_NAME"/>
  <ViewAttribute
    Name="DepartmentId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="DepartmentId"
    EntityUsage="HrDepartments"
    AliasName="DEPARTMENT_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.Lab11Bundle"/>
  </ResourceBundle>
  <AttrArray Name="KeyAttributes">
    <Item Value="Rowid"/>
    <Item Value="LocationId"/>
    <Item Value="JobId"/>
    <Item Value="CountryId"/>
  </AttrArray>
</ViewObject>
